{
  "name": "femundfilou/kirby-blaupause",
  "description": "Kirby Blaupause: A Starter Template",
  "type": "project",
  "authors": [
    {
      "name": "Justus Kraft",
      "email": "justus@femundfilou.com",
      "homepage": "https://femundfilou.com"
    }
  ],
  "homepage": "https://femundfilou.com",
  "require": {
    "php": ">=8.0.0 <8.3.0",
    "getkirby/cms": "^4",
    "vlucas/phpdotenv": "^5.5",
    "lukaskleinschmidt/kirby-laravel-vite": "^2.1",
    "femundfilou/kirby-asset-manager": "^1.0@beta",
    "tobimori/kirby-seo": "^0.4.0",
    "getkirby/staticache": "^1.0",
    "getkirby/cli": "^1.1",
    "distantnative/retour-for-kirby": "^5.0",
    "femundfilou/kirby-image-snippet": "^0.1.0"
  },
  "minimum-stability": "beta",
  "config": {
    "vendor-dir": "backend/vendor",
    "allow-plugins": {
      "getkirby/composer-installer": true
    },
    "optimize-autoloader": true
  },
  "extra": {
    "kirby-cms-path": "backend/kirby",
    "kirby-plugin-path": "backend/site/plugins"
  }
}
