{
	"name": "kirby-blaupause",
	"version": "0.0.0",
	"description": "Kirby Blaupause: A Starter Template",
	"main": "",
	"type": "module",
	"scripts": {
		"dev": "concurrently -n svelte,vite 'pnpm:check' 'vite'",
		"build": "pnpm build:vite && pnpm css:purge",
		"build:vite": "vite build",
		"css:purge": "node scripts/purgecss.js || exit 1",
		"icons:build": "node scripts/svg-sprite.js -i frontend/assets/svg/ -o public/icons.svg -p blaupause",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"blocks:dev": "pnpm --filter blaupause-block-previews dev",
		"blocks:build": "pnpm --filter blaupause-block-previews build",
		"release": "release-it"
	},
	"keywords": [],
	"author": "Justus Kraft <justus@femundfilou.de>",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@release-it/bumper": "^7.0.5",
		"@release-it/conventional-changelog": "^10.0.1",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@total-typescript/ts-reset": "^0.6.1",
		"@tsconfig/svelte": "^5.0.4",
		"@types/node": "^22.15.3",
		"browserslist": "^4.24.5",
		"concurrently": "^9.1.2",
		"glob": "^11.0.2",
		"laravel-vite-plugin": "^1.2.0",
		"lightningcss": "^1.29.3",
		"purgecss": "^7.0.2",
		"release-it": "^19.0.2",
		"svelte": "^5.28.2",
		"svelte-check": "^4.1.7",
		"svgo": "^3.3.2",
		"svgstore": "^3.0.1",
		"terser": "^5.39.0",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"vite": "^5.4.19",
		"yargs": "^17.7.2"
	},
	"dependencies": {
		"@unseenco/taxi": "^1.8.0",
		"cleacss": "2.0.0-beta.4",
		"htmx.org": "^1.9.12",
		"lenis": "^1.3.1",
		"unlazy": "^0.12.4"
	},
	"browserslist": [
		"last 2 versions",
		">= 0.4%",
		"not dead",
		"Firefox ESR",
		"not op_mini all",
		"not and_uc > 0"
	],
	"packageManager": "pnpm@10.10.0"
}
